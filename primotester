#include <stdio.h>

int ehPrimo(int n) {
    if (n <= 1) return 0;

    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) return 0;
    }

    return 1;
}
int encontraEnesimo(int n) {
    int cont = 0;
    int numero = 2;

    while (cont < n) {
        if (ehPrimo(numero)) {
            cont++;
        }
        numero++;
    }
    return numero - 1;
}

int main() {
    int n;

    printf("Digite o valor de n para encontrar o enesimo numero primo: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("O valor de n deve ser um nÃºmero inteiro positivo.\n");
        return 1;
    }

    int enesimoPrimo = encontraEnesimo(n);
    printf("O %d numero primo eh %d.\n", n, enesimoPrimo);

    return 0;
}
